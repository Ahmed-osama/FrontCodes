            var post = {
              img:data.content[i].img,
              caption:data.content[i].title,
              abstract:false,
              icon:{
                state:true,
                name:iconType[i]
              },
              card:true,
              cardBounded:true,
              cardBoundedSimple:true,
              horizontal:false,
              date:false
            }
          .col-sm-6.col-md-3
            include ../components/_basic-post-view.pug
        .clearfix
      
